N/*
N *  Audio Device Class Interface Get Request Callback
N *   Called automatically on ADC Interface Get Request
N *    Parameters:      None (global SetupPacket and EP0Buf)
N *    Return Value:    TRUE - Success, FALSE - Error
N */
N
Nuint32_t ADC_IF_GetRequest (void) {
N
N/*
N  Interface = SetupPacket.wIndex.WB.L;
N  EntityID  = SetupPacket.wIndex.WB.H;
N  Request   = SetupPacket.bRequest;
N  Value     = SetupPacket.wValue.W;
N  ...
N*/
N
N  if (SetupPacket.wIndex.W == 0x0200) {
N    /* Feature Unit: Interface = 0, ID = 2 */
N    if (SetupPacket.wValue.WB.L == 0) {
N      /* Master Channel */
N      switch (SetupPacket.wValue.WB.H) {
N        case AUDIO_MUTE_CONTROL:
X        case 0x01:
N          switch (SetupPacket.bRequest) {
N            case AUDIO_REQUEST_GET_CUR:
X            case 0x81:
N              EP0Buf[0] = Mute;
N              return (TRUE);
X              return ((1));
N          }
N          break;
N        case AUDIO_VOLUME_CONTROL:
X        case 0x02:
N          switch (SetupPacket.bRequest) {
N            case AUDIO_REQUEST_GET_CUR:
X            case 0x81:
N              *((__packed uint16_t *)EP0Buf) = VolCur;
N              return (TRUE);
X              return ((1));
N            case AUDIO_REQUEST_GET_MIN:
X            case 0x82:
N              *((__packed uint16_t *)EP0Buf) = VolMin;
N              return (TRUE);
X              return ((1));
N            case AUDIO_REQUEST_GET_MAX:
X            case 0x83:
N              *((__packed uint16_t *)EP0Buf) = VolMax;
N              return (TRUE);
X              return ((1));
N            case AUDIO_REQUEST_GET_RES:
X            case 0x84:
N              *((__packed uint16_t *)EP0Buf) = VolRes;
N              return (TRUE);
X              return ((1));
N          }
N          break;
N      }
N    }
N  }
N  return (FALSE);  /* Not Supported */
X  return ((0));   
N}
N
N
N/*
N *  Audio Device Class Interface Set Request Callback
N *   Called automatically on ADC Interface Set Request
N *    Parameters:      None (global SetupPacket and EP0Buf)
N *    Return Value:    TRUE - Success, FALSE - Error
N */
N
Nuint32_t ADC_IF_SetRequest (void) {
N
N/*
N  Interface = SetupPacket.wIndex.WB.L;
N  EntityID  = SetupPacket.wIndex.WB.H;
N  Request   = SetupPacket.bRequest;
N  Value     = SetupPacket.wValue.W;
N  ...
N*/
N
N  if (SetupPacket.wIndex.W == 0x0200) {
N    /* Feature Unit: Interface = 0, ID = 2 */
N    if (SetupPacket.wValue.WB.L == 0) {
N      /* Master Channel */
N      switch (SetupPacket.wValue.WB.H) {
N        case AUDIO_MUTE_CONTROL:
X        case 0x01:
N          switch (SetupPacket.bRequest) {
N            case AUDIO_REQUEST_SET_CUR:
X            case 0x01:
N              Mute = EP0Buf[0];
N              return (TRUE);
X              return ((1));
N          }
N          break;
N        case AUDIO_VOLUME_CONTROL:
X        case 0x02:
N          switch (SetupPacket.bRequest) {
N            case AUDIO_REQUEST_SET_CUR:
X            case 0x01:
N              VolCur = *((__packed uint16_t *)EP0Buf);
N              return (TRUE);
X              return ((1));
N          }
N          break;
N      }
N    }
N  }
N  return (FALSE);  /* Not Supported */
X  return ((0));   
N}
